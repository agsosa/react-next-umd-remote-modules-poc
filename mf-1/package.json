    "@types/react-dom": "^19",
    "@types/styled-components": "^5.1.34",
    "@vitejs/plugin-react": "^4.7.0",
    "concurrently": "^9.2.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.4",
    "typescript": "^5",
    "vite": "^7.0.6"
  }
